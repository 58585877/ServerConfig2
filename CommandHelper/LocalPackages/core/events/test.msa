        'name': @hero['name'],
        'color': @hero['chatcolor']
    )

    array_remove(@hero,'name')
    array_remove(@hero,'chatcolor')

    write_file('/home/minecraft/server/plugins/CommandHelper/LocalPackages/commands/chars/'.@class.'.yml',yml_encode(@hero,true),'OVERWRITE')

